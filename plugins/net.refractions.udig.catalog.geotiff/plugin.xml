<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
           id="net.refractions.udig.catalog.geotiff.services"
           name="GeoTiff Services"
           point="net.refractions.udig.catalog.ServiceExtension">
      <service
            class="net.refractions.udig.catalog.internal.geotiff.GeoTiffServiceExtension"
            id="GeoTiff"
            name="GeoTiff"/>
   </extension>
   <extension
         id="net.refractions.udig.catalog.geotiff.fileFormats"
         name="GeoTiff File Formats"
         point="net.refractions.udig.catalog.ui.fileFormat">
      <fileService
            fileExtension="*.tif"
            name="Tif"/>
      <fileService
            fileExtension="*.tiff"
            name="Tiff"/>
   </extension>

</plugin>
