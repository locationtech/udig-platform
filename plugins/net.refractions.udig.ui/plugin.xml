<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>


   <extension-point id="dataWizards" name="%dataWizards.name" schema="schema/dataWizards.exsd"/>
   <extension-point id="operation" name="operation" schema="schema/operation.exsd"/>
   <extension-point id="newObjectAction" name="newObjectAction" schema="schema/newObjectAction.exsd"/>
   <extension-point id="dropAction" name="Drop Action" schema="schema/dropAction.exsd"/>
   <extension-point id="dropTransfers" name="Drop Transfers" schema="schema/dropTransfer.exsd"/>
   <extension-point id="tip" name="tip" schema="schema/tip.exsd"/>
   <extension-point id="objectProperty" name="%objectProperty.point" schema="schema/objectProperty.exsd"/>
   <extension-point id="workbenchConfigurations" name="%workbenchConfigurations" schema="schema/workbenchConfigurations.exsd"/>
   <extension-point id="menuBuilders" name="%menuBuilders" schema="schema/menuBuilders.exsd"/>
   <extension-point id="boundary" name="Boundary" schema="schema/boundary.exsd"/>

   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="net.refractions.udig.ui.preferences.PreferenceInitializer"/>
   </extension>

	<extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%mapPerspective"
            icon="icons/obj16/map_obj.gif"
            class="net.refractions.udig.internal.ui.MapPerspective"
            id="net.refractions.udig.ui.mapPerspective">
      </perspective>
      <perspective
            class="net.refractions.udig.internal.ui.StylePerspective"
            fixed="false"
            icon="icons/obj16/map_obj.gif"
            id="net.refractions.udig.ui.stylePerspective"
            name="%style.perspective.name"/>
   </extension>
      <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="net.refractions.udig.ui.page.general"
            class="net.refractions.udig.ui.preferences.UiPreferences"
            id="net.refractions.udig.ui.preferences"
            name="%uiPreferences">
         <keywordReference
               id="charset">
         </keywordReference>
      </page>
      <page
            category="net.refractions.udig.ui.page.general"
            class="net.refractions.udig.ui.preferences.RuntimePreferences"
            id="net.refractions.udig.ui.runntimepreferences"
            name="%runntimePreferences">
         <keywordReference
               id="charset">
         </keywordReference>
      </page>
       </extension>
   <extension
         id="application"
         name="%application.name"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.refractions.udig.internal.ui.UDIGApplication">
         </run>
      </application>
   </extension>
   
  
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Progress View"
            category="org.eclipse.ui"
            class="org.eclipse.ui.ExtensionFactory:progressView"
            id="org.eclipse.ui.views.ProgressView">
      </view>
      <view
            category="catalog"
            class="net.refractions.udig.ui.boundary.BoundaryView"
            id="net.refractions.udig.ui.viewBoundary"
            name="%boundary.name"
            restorable="true">
      </view>
   </extension>
	<extension
			point="org.eclipse.ui.actionSets">
		<actionSet
				label="%HelpActionSet.label"
				visible="false"
				id="net.refractions.udig.helpMenuItems">
				<menu
         id="net.refractions.udig.updateMenu"
         label="%UpdateActionSet.menu.label"
         path="help/helpEnd">
			</menu>
				<action
         class="net.refractions.udig.InstallWizardAction"
         disabledIcon="icons/dtool16/usearch_obj.gif"
         icon="icons/etool16/usearch_obj.gif"
         id="net.refractions.udig.updateMenu.newUpdates"
         label="%UpdateActionSet.updates.label"
         menubarPath="help/helpEnd">
			</action>
   <action
         class="net.refractions.udig.internal.ui.ShowSubmitIssueAction"
         id="net.refractions.udig.ui.submitIssueAction"
         label="%log.action.name"
         menubarPath="help/helpEnd"
         style="push"
         tooltip="%log.action.tip"/>
   <action
         class="net.refractions.udig.internal.ui.ShowTipsAction"
         id="net.refractions.udig.ui.tips.action"
         label="%tips.action.name"
         menubarPath="help/helpEnd"
         style="push"/>
		</actionSet>
  <actionSet
        description="Default Actions"
        id="net.refractions.udig.ui.default"
        label="%DefaultActionSet.label"
        visible="false">
     <action
           class="net.refractions.udig.ui.operations.OtherAction"
           id="net.refractions.udig.ui.otherAction"
           label="%Other.Operations"
           menubarPath="edit/other">
     </action>
  </actionSet>
	</extension>
    <extension
         point="net.refractions.udig.ui.dropTransfers">
      <dropTransfer class="net.refractions.udig.internal.ui.UDigTransferFactory"/>
      <!--dropTransfer class="net.refractions.udig.internal.ui.UDIGFileTransfer"/>
      <dropTransfer class="net.refractions.udig.internal.ui.UDIGTextTransfer"/>
      <dropTransfer class="net.refractions.udig.internal.ui.UDIGRTFTransfer"/-->
   </extension>
    <extension
          point="org.eclipse.ui.startup">
       <startup class="net.refractions.udig.ui.internal.DeveloperStartup"/>
       <startup
             class="net.refractions.udig.internal.ui.UDIGStartup">
       </startup>
    </extension>
    <extension
          point="org.eclipse.ui.preferencePages">
       <page
             category="net.refractions.udig.ui.page.general"
             class="org.eclipse.ui.internal.keys.KeysPreferencePage"
             id="net.refractions.udig.ui.keys"
             name="%key.preferences"/>
       <page
             category="net.refractions.udig.ui.page.general"
             class="net.refractions.udig.internal.ui.TransferPreference"
             id="net.refractions.udig.ui.transfer.preference"
             name="%net.refractions.udig.ui.transfer.preference.name"/>
       <page
             class="net.refractions.udig.internal.ui.GeneralPreferences"
             id="net.refractions.udig.ui.page.general"
             name="%general.preference.name"/>
    </extension>
    <extension
          point="net.refractions.udig.ui.operation">
               <category
           id="net.refractions.udig.ui.edit"
           name="%EditCategory"/>
    </extension>
    <extension
          point="org.eclipse.ui.bindings">
       <scheme
             description="%defaultKeyConfiguration.desc"
             id="net.refractions.udig.defaultUDIGKeyConfiguration"
             name="%defaultKeyConfiguration.name"/>
       <key
             commandId="org.eclipse.ui.file.close"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+W"/>
       <key
             commandId="org.eclipse.ui.file.save"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+S"/>    
       <key
             commandId="org.eclipse.ui.file.print"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+P"/>    
       <key
             commandId="org.eclipse.ui.file.properties"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+ENTER"/>    
       <key
             commandId="org.eclipse.ui.edit.undo"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+Z"/>    
       <key
             commandId="org.eclipse.ui.edit.redo"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+Y"/>    
       <key
             commandId="org.eclipse.ui.edit.cut"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+X"/>    
       <key
             commandId="org.eclipse.ui.edit.copy"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+C"/>    
       <key
             commandId="org.eclipse.ui.edit.paste"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+V"/>    
       <key
             commandId="org.eclipse.ui.edit.delete"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="DELETE"/>    

       <key
             commandId="org.eclipse.ui.edit.selectAll"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+A"/>    
       <key
             commandId="org.eclipse.ui.edit.addBookmark"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="M1+D"/>    
       <key
             commandId="org.eclipse.ui.navigate.back"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="ALT+ARROW_LEFT"/>    
       <key
             commandId="org.eclipse.ui.navigate.forward"
             contextId="org.eclipse.ui.contexts.dialogAndWindow"
             schemeId="net.refractions.udig.defaultUDIGKeyConfiguration"
             sequence="ALT+ARROW_RIGHT"/>    
       </extension>
    <extension
          id="net.refractions.udig.ui.tip.default"
          point="net.refractions.udig.ui.tip">
       <tip
             id="net.refractions.udig.ui.keyZoom"
             name="%zoom.tip.name">
          %zoom.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.keyPan"
             name="%pan.tip.name">
          %pan.tip.tip 
       </tip>
       <tip
             id="net.refractions.udig.ui.zoomExtent"
             name="%showall.tip.name">
          %showall.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.antiAliasing"
             name="%antialiasing.tip.name">
          %antialiasing.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.background.color"
             name="%background.color.tip.name">
          %background.color.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.transparency"
             name="%transparency.tip.name">
          %transparency.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.projection"
             name="%projection.tip.name">
          %projection.tip.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.DeleteVertex"
             name="%tip.delete.vertex.name">
          %tip.delete.vertex.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.snap.size"
             name="%tip.snap.size.name">
          %tip.snap.size.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.change.location"
             name="%tip.change.location.name">
          %tip.change.location.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.change.location.ll"
             name="%tip.change.location.ll.name">
          %tip.change.location.ll.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.tip.mylar"
             name="%tip.mylar.name">
          %tip.mylar.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.copy.feature"
             name="%tip.copy.feature.name">
          %tip.copy.feature.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.export.feature"
             name="%tip.export.feature.name">
          %tip.export.feature.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.export.filter"
             name="%tip.export.filter.name">
          %tip.export.filter.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.bookmarks"
             name="%tip.bookmarks.name">
          %tip.bookmarks.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.extend.line"
             name="%tip.extend.line.name">
          %tip.extend.line.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.styling.1"
             name="%styling.1.name">
          %styling.1.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.styling.2"
             name="%styling.2.name">
          %styling.2.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.styling.3"
             name="%styling.3.name">
          %styling.3.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.validation.1"
             name="%validation.1.name">
          %validation.1.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.split.line"
             name="%tip.split.line.name">
          %tip.split.line.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.snapping.hotkey"
             name="%tip.snapping.hotkey.name">
          %tip.snapping.hotkey.tip
       </tip>
       <tip
             id="net.refractions.udig.ui.tip.advanced.editing.hotkey"
             name="%tip.advanced.editing.hotkey.name">
          %tip.advanced.editing.hotkey.tip
       </tip>
    </extension>
    <extension
          point="org.eclipse.ui.perspectiveExtensions">
       <perspectiveExtension targetID="net.refractions.udig.ui.mapPerspective">
          <actionSet id="net.refractions.udig.helpMenuItems"/>
       </perspectiveExtension>
       <perspectiveExtension targetID="net.refractions.udig.ui.stylePerspective">
          <actionSet id="net.refractions.udig.helpMenuItems"/>
       </perspectiveExtension>
    </extension>
    <extension
          point="org.eclipse.ui.keywords">
       <keyword
             id="charset"
             label="%charset">
       </keyword>
    </extension>
    <extension
          point="org.eclipse.ui.services">
       <serviceFactory
             factoryClass="net.refractions.udig.internal.boundary.BoundaryServiceFactory">
          <service
                serviceClass="net.refractions.udig.boundary.IBoundaryService">
          </service>
       </serviceFactory>
    </extension>
    <extension
          id="net.refractions.udig.ui.boundaryStrategy"
          name="Default Boundary"
          point="net.refractions.udig.ui.boundary">
       <boundary
             class="net.refractions.udig.internal.boundary.BoundaryStrategyAll"
             id="net.refractions.udig.ui.boundaryAll"
             name="All">
       </boundary>
    </extension>
    <extension
          id="net.refractions.udig.ui.boundaryProperty"
          point="net.refractions.udig.ui.objectProperty">
       <object
             targetClass="java.lang.Object">
          <property
                class="net.refractions.udig.internal.boundary.BoundaryStrategyPropertyValue"
                id="boundaryStrategy">
          </property>
       </object>
    </extension>
</plugin>
